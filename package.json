{
  "name": "lens-vault",
  "private": true,
  "version": "1.0.0",
  "description": "Secure password manager with satellite verification",
  "author": "Lens Vault Ltd",
  "main": "electron-main.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "server": "tsx --watch server/index.ts",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "NODE_ENV=development electron .",
    "electron:build": "npm run build && electron-builder --win",
    "electron:build:dir": "npm run build && electron-builder --win --dir"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@google/genai": "^1.28.0",
    "@supabase/supabase-js": "^2.47.10",
    "axios": "^1.13.2",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.552.0",
    "nodemon": "^3.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-paystack": "^6.0.0",
    "recharts": "^3.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.1",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "build": {
    "appId": "com.lensvault.app",
    "productName": "LensVault",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron-main.js",
      "preload.js",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}